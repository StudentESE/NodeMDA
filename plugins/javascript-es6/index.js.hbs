
{{#with _jsNamespaceQueue as |namespaceQueue|}}
{{#each namespaceQueue.packageNames as |pkgName|}}
{{initJSNamespace pkgName}}
	{{#each (namespaceQueue.packageClassList pkgName) as |class|}}
	{{pkgName}}.{{class.name}} = require('./{{class.packageDirName}}/{{class.name}}.js');
	{{#if class.hasOperations}}
	    require('./{{class.packageDirName}}/{{class.name}}-operations.js');
	{{/if}}
	{{/each}}
{{/each}}
{{/with}}
